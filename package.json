{
  "name": "reablocks-figma-plugin",
  "author": "Good Code, LLC",
  "license": "Apache-2.0",
  "homepage": "https://github.com/reaviz/reablocks-figma-plugin#readme",
  "version": "2.0.0",
  "description": "A Figma plugin to generate CSS tokens for reablocks.",
  "keywords": [
    "figma",
    "figma-plugin",
    "reablocks",
    "design-tokens",
    "css-variables"
  ],
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.3",
    "@create-figma-plugin/utilities": "^4.0.3",
    "change-case": "^5.4.4",
    "chroma-js": "^2.4.2",
    "copy-to-clipboard": "^3.3.3",
    "preact": "^10.22.0",
    "prismjs": "^1.28.0",
    "react-simple-code-editor": "^0.13.1"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@figma/plugin-typings": "^1.117.0",
    "@types/chroma-js": "^2.4.4",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.2",
    "react": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma",
      "dev"
    ],
    "capabilities": [
      "inspect"
    ],
    "documentAccess": "dynamic-page",
    "id": "reablocks-figma-plugin",
    "name": "Reablocks Figma Plugin",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
